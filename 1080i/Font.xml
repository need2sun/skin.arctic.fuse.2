            <param name="font_light">Inter-Light.ttf</param>
            <param name="plot_linespacing_head">1.1</param>
            <param name="plot_linespacing_midi">1.35</param>
            <param name="plot_linespacing_main">1.107</param>
            <param name="plot_linespacing_mini">1.205</param>
            <param name="plot_linespacing_tiny">1.07</param>
        </include>
